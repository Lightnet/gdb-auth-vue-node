{
  "//1": "describes your app and its dependencies",
  "//2": "https://docs.npmjs.com/files/package.json",
  "//3": "updating this file will download and update your packages",
  "name": "gdb-auth-vue-node",
  "version": "0.0.2",
  "description": "A simple Node app with gundb as a database management system, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "start": "webpack && babel-node main.js --presets env --host 0.0.0.0",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-component": "1.1.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "browser-sync": "2.24.4",
    "css-loader": "0.28.11",
    "dotenv": "5.0.1",
    "ejs": "2.6.1",
    "element-ui": "2.4.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "gulp": "3.9.1",
    "gulp-live-server": "0.0.31",
    "gun": "0.9.998",
    "helmet": "3.12.1",
    "node-sass": "4.9.0",
    "raw-loader": "0.5.1",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "vue": "2.5.16",
    "vue-gun": "0.0.3",
    "vue-loader": "14.2.3",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.11.0",
    "webpack-cli": "2.1.5",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-sqlite"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {}
        }
      ]
    ]
  },
  "keywords": [
    "node",
    "glitch",
    "express",
    "gundb"
  ]
}
